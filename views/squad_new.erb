<%= erb :'partials/_header' %>
<h1>Add a new squad</h1>
  <form action="/squads" method="POST">
    <label>Name: </label>
    <input type="text" name="name" id="name" autofocus>
    <label>Mascot: </label>
    <input type="text" name="mascot" id="mascot">
    <input type="submit" value="Add!">
  </form>
  <a href="/squads">Back</a>
<%=erb :'partials/_footer'%>